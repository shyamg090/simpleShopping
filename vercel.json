{
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        },   
        {
            "src": "*build/",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/products",
            "dest": "index.js"
        },           
        {
            "src": "/products/(.*)",
            "dest": "index.js"
        },     
        {
            "src": "*build/",
            "dest": "build/"
        }
    ]
}